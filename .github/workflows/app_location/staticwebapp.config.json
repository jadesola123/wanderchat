{
  "routes": [
    {
      "route": "/memes",
      "rewrite": "/api/memes"
    },
    {
      "route": "/*",
      "serve": "/public/index.html",
      "statusCode": 404
    }
  ],
  "platform": {
    "apiRuntime": "node:16"
  }
  "navigationFallback": {
    "rewrite": "/public/index.html",
    "exclude": ["/api/*"]
  },
  "httpHeaders": {
    "/api/*": [
      {
        "name": "Cache-Control",
        "value": "no-store"
      }
    ],
    "/*.html": [
      {
        "name": "Cache-Control",
        "value": "no-cache"
      }
    ]
  },
  "mimeTypes": {
    "text/markdown": ["md"],
    "image/webp": ["webp"]
  }
}
